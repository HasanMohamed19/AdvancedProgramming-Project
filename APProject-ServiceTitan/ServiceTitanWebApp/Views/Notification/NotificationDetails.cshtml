@model ServiceTitanBusinessObjects.Notification

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>NotificationDetails</title>
</head>
<body>

<div>
    <h4>Notification</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.NotificationMessage)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NotificationMessage)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.NotificationTitle)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NotificationTitle)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.NotificationType)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NotificationType)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.NotificationStatus)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NotificationStatus.NotificationStatusName)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.User)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.User.Password)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.NotificationID">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
</body>
</html>
